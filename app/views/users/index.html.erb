<h2> Users </h2>

<table>
    <tr>
        <th>Id</th>
        <th>Email</th>
        <th>Role</th>
        <th>Create date</th>
    </tr>
    <% @users.each do |user| %>
        <tr>
            <td><%= user.id %></td>
            <td><%= user.email %></td>
            <td><%= user.role %></td>
            <td><%= user.created_at %></td>
        </tr>
    <% end %>
</table>